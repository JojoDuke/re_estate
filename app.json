{
  "expo": {
    "name": "reestate-project",
    "slug": "reestate-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MDSplash4.png",
    "splash": {
      "image": "./assets/images/MDSplash4.png",
      "imageWidth": 20,
      "resizeMode": "contain",
      "backgroundColor": "#1AB02A",
      "enableFullScreenImage_legacy": true,
      "androidStatusBar": {
        "backgroundColor": "#1AB02A",
        "translucent": false,
        "barStyle": "dark-content"
      }
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reestate.project"
    },
    "android": {
      "package": "com.reestate.project",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/japan.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET"
      ],
      "splash": {
        "backgroundColor": "#1AB02A",
        "resizeMode": "cover",
        "androidStatusBar": {
          "backgroundColor": "#1AB02A",
          "translucent": false
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/MDSplash4.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#1AB02A",
          "enableFullScreenImage_legacy": true,
          "androidStatusBar": {
            "backgroundColor": "#1AB02A",
            "translucent": false,
            "barStyle": "dark-content"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Rubik-Regular.ttf",
            "assets/fonts/Rubik-Bold.ttf",
            "assets/fonts/Rubik-ExtraBold.ttf",
            "assets/fonts/Rubik-Light.ttf",
            "assets/fonts/Rubik-Medium.ttf",
            "assets/fonts/Rubik-SemiBold.ttf",
            "assets/fonts/PlusJakartaSans-Regular.ttf",
            "assets/fonts/PlusJakartaSans-Medium.ttf",
            "assets/fonts/PlusJakartaSans-Bold.ttf",
            "assets/fonts/PlusJakartaSans-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "androidStatusBar": {
      "backgroundColor": "#1AB02A",
      "translucent": false,
      "barStyle": "dark-content"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fc8ef9ef-8848-483f-8e7a-29692aa60da6"
      }
    }
  }
}
