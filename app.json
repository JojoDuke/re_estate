{
  "expo": {
    "name": "MiDa Estates",
    "slug": "MiDa-Estates",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "splash": {
      "image": "./assets/images/MDSplash4.png",
      "resizeMode": "cover",
      "backgroundColor": "#1AB02A"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bhyte.midaestates"
    },
    "android": {
      "package": "com.bhyte.midaestates",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#1AB02A"
      },
      "splash": {
        "image": "./assets/images/MDSplash4.png",
        "resizeMode": "cover",
        "backgroundColor": "#1AB02A"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/MDSplash4.png",
          "resizeMode": "cover",
          "backgroundColor": "#1AB02A"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Rubik-Regular.ttf",
            "assets/fonts/Rubik-Bold.ttf",
            "assets/fonts/Rubik-ExtraBold.ttf",
            "assets/fonts/Rubik-Light.ttf",
            "assets/fonts/Rubik-Medium.ttf",
            "assets/fonts/Rubik-SemiBold.ttf",
            "assets/fonts/PlusJakartaSans-Regular.ttf",
            "assets/fonts/PlusJakartaSans-Medium.ttf",
            "assets/fonts/PlusJakartaSans-Bold.ttf",
            "assets/fonts/PlusJakartaSans-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fc8ef9ef-8848-483f-8e7a-29692aa60da6"
      }
    }
  }
}
