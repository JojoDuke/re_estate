{
  "expo": {
    "name": "reestate-project",
    "slug": "reestate-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MDSplash.png",
    "splash": {
      "image": "./assets/images/MDSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFF024",
      "enableFullScreenImage_legacy": true,
      "androidStatusBar": {
        "backgroundColor": "#FFF024",
        "translucent": false,
        "barStyle": "dark-content"
      }
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reestate.project"
    },
    "android": {
      "package": "com.reestate.project",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/japan.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.INTERNET"],
      "splash": {
        "backgroundColor": "#FFF024",
        "resizeMode": "cover",
        "androidStatusBar": {
          "backgroundColor": "#FFF024",
          "translucent": false
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#FFF024",
          "enableFullScreenImage_legacy": true,
          "androidStatusBar": {
            "backgroundColor": "#FFF024",
            "translucent": false,
            "barStyle": "dark-content"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Rubik-Regular.ttf",
            "assets/fonts/Rubik-Bold.ttf",
            "assets/fonts/Rubik-ExtraBold.ttf",
            "assets/fonts/Rubik-Light.ttf",
            "assets/fonts/Rubik-Medium.ttf",
            "assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "androidStatusBar": {
      "backgroundColor": "#FFF024",
      "translucent": false,
      "barStyle": "dark-content"
    }
  }
}
